{
  "extends": ["plugin:@next/next/recommended", "plugin:prettier/recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "sourceType": "module",
    "babelOptions": {
      "presets": ["next/babel"]
    }
  },
  "plugins": ["prettier"],
  "rules": { "prettier/prettier": ["error", {}, { "usePrettierrc": true }] }
}
